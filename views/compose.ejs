
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Subject</label>
        <input class="form-control" type="text" name="postSubject">
        <label>Question</label>
        <textarea class="form-control" name="postQuestion" rows="5" cols="30"></textarea>
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
        <label>Summary</label>
        <textarea class="form-control" name="postSummary" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
